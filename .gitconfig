[core]
    editor = vim
[user]
    name = Robin Schroer
    email = sulamiification@gmail.com
[push]
    default = simple
[credential]
    helper = cache --timeout=86400
[alias]
    st = status
    sh = show
    di = diff
    ds = diff --stat
    dh = diff HEAD --cached
    dhs = diff HEAD --cached --stat
    br = branch
    co = checkout
    add = add --all
    aa = add --all .
    ap = add --all -p
    cm = commit
    cmm = commit -m
    ca = commit -a
    cam = commit -am
    pom = push origin master
    plm = push local master
    pgm = push gh master
    fe = fetch
    puo = pull origin master
    pul = pull local master
    pug = pull gh master
    l = log --decorate --graph --oneline
    ll = log --decorate --graph
[merge]
    tool = vimdiff
[sendemail]
    smtpencryption = tls
    smtpserver = smtp.gmail.com
    smtpuser = sulamiification@gmail.com
    smtpserverport = 587

